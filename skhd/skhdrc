shift + cmd - t : yabai -m window --toggle float;
shift + cmd - s : yabai -m window --toggle topmost;

shift + cmd - b :yabai -m window --toggle border

ctrl + cmd - l : yabai -m space --focus next || yabai -m space --focus first
ctrl + cmd - h : yabai -m space --focus prev || yabai -m space --focus last

cmd - l : yabai -m window --focus east
cmd - h : yabai -m window --focus west
cmd - k : yabai -m window --focus north
cmd - j : yabai -m window --focus south

cmd - 1 : yabai -m space --focus 1
cmd - 2 : yabai -m space --focus 2
cmd - 3 : yabai -m space --focus 3
cmd - 4 : yabai -m space --focus 4
cmd - 5 : yabai -m space --focus 5

ctrl + cmd - 1 : yabai -m window --space 1 && yabai -m space --focus 1
ctrl + cmd - 2 : yabai -m window --space 2 && yabai -m space --focus 2

.blacklist [
    "terminal"
]
